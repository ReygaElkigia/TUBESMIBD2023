<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="../HomePageStyle.css">
  <script src="https://kit.fontawesome.com/985f81e0fc.js" crossorigin="anonymous"></script>
  <title>My Spotiti</title>
</head>

<body>
  <div class="container">
    <div class="column column-1">

      
        <% if(status == "Membership"){ %>
          
          <%- include('sideBarMembership') %>
        <% } else{ %>
          <%- include('sideBar') %>
        <% } %>
         
      
    </div>
    <div class="column column-2">
      
      <div class="made-for-you">
        
        <div class="image-container">
          <a href="#">
            
          </a>
        </div>
        <div><br></div>
        <h2>Already Membership!</h2>
        <ul class="song-list">
            <table>
              <thead>
                <tr>
                  <th>id Membership</th>
                  <th>Packet</th>
                  <th>Tanggal Mulai</th>
                  <th>Tanggal berakhir</th>
                </tr>
              </thead>
              <tbody>
                <% let i=1 %>
                <% for(let row of paket) { %>
                  <tr>
                    <td>
                      <%= row.id_membership %>
                    </td>
                    <td>
                      <%= row.subscription %>
                    </td>
                    <td>
                        <% const tanggalMulai = new Date(row.tanggal_mulai); %>
                        <%= tanggalMulai.toISOString().slice(0, 10) %>
                    </td>
                    <td>
                        <% const tanggalAkhir = new Date(row.tanggal_akhir); %>
                        <%= tanggalAkhir.toISOString().slice(0, 10) %>
                      </td>
                  </tr>
                  <% i++ %>
                    <% } %>

              </tbody>
            </table>

            <!-- Tambahkan lebih banyak item lagu sesuai keinginan -->
        </ul>
      </div>
    </div>
    <div class="column column-3">
      <%- include('sideBar2') %>
    </div>
  </div>
</body>

</html>
